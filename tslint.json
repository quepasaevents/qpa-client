{
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "member-access": false,
    "object-literal-sort-keys":false,
    "no-console": false,
    "interface-name" : false,
    "no-empty-interface" : false,
    "max-classes-per-file" : false,
    "semicolon": [true, "never"]
  }
}
