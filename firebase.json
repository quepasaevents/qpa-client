{
  "functions": {
    "predeploy": [
    ]
  },
  "hosting": {
    "rewrites": [
      {
        "source": "/api/isUserAvailable",
        "function": "isUserAvailable"
      },
      {
        "source": "/api/signup",
        "function": "signup"
      },
      {
        "source": "/api/signin",
        "function": "signin"
      },
      {
        "source": "/api/session",
        "function": "postSession"
      },
      {
        "source": "/events",
        "function": "events"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "public": "webapp/dist"

  }
}
