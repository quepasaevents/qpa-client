{
  "name": "functions",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "build": "tsc --version && tsc",
    "serve": "npm run build && firebase serve --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "gcloud beta functions deploy signin --trigger-http; gcloud beta functions deploy isUserAvailable --trigger-http; gcloud beta functions deploy signup --trigger-http",
    "logs": "firebase functions:log"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/datastore": "^1.4.0",
    "@types/node-fetch": "^2.1.1",
    "atob": "^2.1.1",
    "axios": "^0.18.0",
    "cookie": "^0.3.1",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "firebase-admin": "~5.12.0",
    "firebase-functions": "^2.0.5",
    "google-auth-library": "^1.5.0",
    "googleapis": "^31.0.2",
    "joi": "^13.6.0",
    "joi-phone-number": "^2.0.12",
    "joi-timezone": "^2.0.0",
    "jsonwebtoken": "^8.2.1",
    "mailgun-js": "^0.16.0",
    "node-fetch": "^2.1.2",
    "node-pre-gyp": "^0.9.0",
    "random-string": "^0.2.0",
    "randomstring": "^1.1.5",
    "superagent": "^3.8.3",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/express": "^4.11.1",
    "@types/gapi.client.calendar": "^3.0.0",
    "@types/google-cloud__datastore": "^1.3.1",
    "@types/joi": "^13.0.8",
    "@types/node": "^9.6.1",
    "babel-loader": "^7.1.4",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "2.9.2",
    "typescript-loader": "^1.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "modulePathIgnorePatterns": [
      ".png",
      ".scss"
    ],
    "testPathIgnorePatterns": [
      "node_modules"
    ],
    "testRegex": "/__tests__/.*spec.tsx?$"
  },
  "private": true,
  "version": "1.0.0",
  "license": "MIT"
}
